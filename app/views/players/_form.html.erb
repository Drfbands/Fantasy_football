<%= form_for([@team, @team.players.build]) do |f| %>
	<%= f.text_field :name, placeholder: "New Name" %><br>
	<%= f.text_field :position, placeholder: "Position" %><br>
	<%= f.text_area :notes, placeholder: "Notes on this player" %><br>
	<%= f.text_area :why, placeholder: "Why will he be good?" %><br>
	<%= f.text_area :why_not, placeholder: "Why could he not be good?" %><br>
	<%= f.submit %>
<% end %>